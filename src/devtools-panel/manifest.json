{
  "manifest_version": 3,
  "name": "Twine Dugger",
  "version": "$version",
  "description": "A tool to inspect and diff game state for Twine games.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>", "file:///*"],
  "devtools_page": "./create-panel.html",
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": ["./content-script.js"],
      "matches": ["<all_urls>", "file:///*"]
    }
  ]
}
